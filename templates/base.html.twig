            <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
                <li class="nav-item active">
                    <a class="nav-link" href="https://127.0.0.1:8000/default">Home <span class="sr-only">(current)</span></a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="https://127.0.0.1:8000/blog">Blog</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="https://127.0.0.1:8000/login">Inlog</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="https://127.0.0.1:8000/register">Registreren</a>
                </li>
            </ul>
        </div>
    </nav>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
